{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "style-guide-block/style-guide",
	"version": "0.1.0",
	"title": "Style Guide",
	"category": "widgets",
	"icon": "smiley",
	"description": "A simple block for displaying your theme.json styles.",
	"example": {},
	"supports": {
		"html": false,
		"anchor": true
	},
	"attributes": {
		"selectedSections": {
			"type": "array",
			"default": [
				"color-palette",
				"font-sizes",
				"spacing-scale",
				"shadows"
			]
		}
	},
	"textdomain": "style-guide",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
